[Unit]
Description=Gunicorn service for exorsict(Dev site) put this to /etc/systemd/system
After=network.target

[Service]
User=exlinux_dev
WorkingDirectory=/root/exorcist_dev/storytell
ExecStart=/root/exorcist_dev/storytell/gunicornenv/bin/gunicorn -b 127.0.0.1:8001 exorcist.wsgi
Restart=on-failure

[Install]
WantedBy=multi-user.target